(define (morph id texture-name probability active can-be-toxic wing-shape)
  (list id texture-name probability active can-be-toxic wing-shape))

(define (morph-id m) (list-ref m 0))
(define (morph-texture-name m) (list-ref m 1))
(define (morph-probability  m) (list-ref m 2))
(define (morph-active m) (list-ref m 3))
(define (morph-can-be-toxic m) (list-ref m 4))
(define (morph-wing-shape  m) (list-ref m 5))
